    println("fmod(7.5, 2.0):", fmod(7.5, 2.0));
    println("gcd(48, 18):", gcd(48, 18));
    println("lcm(21, 6):", lcm(21, 6));
    println("factorial(6):", factorial(6));
    println("nPr(5,3):", nPr(5,3));
    println("nCr(5,3):", nCr(5,3));
