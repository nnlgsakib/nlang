import std;
def main() {
    println("=== math std lib demo ===");
    println("pi:", pi());
    println("e:", e());
    println("ln(1):", ln(1.0));
    println("ln(e):", ln(e()));
    println("exp(1):", exp(1.0));
    println("log10(1000):", log10(1000.0));
    println("log2(8):", log2(8.0));
    println("sqrt(2):", sqrt(2.0));
    println("pow_float(2, 3.5):", pow_float(2.0, 3.5));
    println("sin(pi/6):", sin(pi() / 6.0));
    println("cos(pi/3):", cos(pi() / 3.0));
    println("tan(pi/4):", tan(pi() / 4.0));
    println("atan(1):", atan(1.0));
    println("asin(0.5):", asin(0.5));
    println("acos(0.5):", acos(0.5));
    println("floor(3.7):", floor(3.7));
