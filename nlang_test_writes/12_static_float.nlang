// Static float tests: f32/f64 types and scientific notation

def main() {
    store a: f32 = 3.14;
    store b: f64 = 2.718281828;
    store c = 1.2e3; // defaults to f64

    store s: f32 = a + 2; // f32 + int -> f64, assign to f32
    store d: f64 = a + b; // f32 + f64 -> f64
    store f: f32 = 1.0e-3; // scientific notation literal assigned to f32

    println(a);
    println(b);
    println(c);
    println(s);
    println(d);
    println(f);
}